{
  "name": "@purr/eslint-config",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "./base": "./base.js"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@purr/prettier-config",
  "dependencies": {
    "@eslint/compat": "^1.2.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-turbo": "^2.3.3",
    "typescript-eslint": "^8.19.1"
  },
  "devDependencies": {
    "@purr/prettier-config": "workspace:*",
    "@purr/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
